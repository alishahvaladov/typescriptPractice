{
    "name": "crack-coding",
    "version": "1.0.0",
    "main": "src/server.ts",
    "license": "MIT",
    "devDependencies": {
        "@ef-carbon/tspm": "^2.2.5",
        "@types/express": "^4.17.20",
        "@types/node": "^20.8.9",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "mongoose": "^7.6.3"
    },
    "scripts": {
        "build": "tsc",
        "start": "node ./bin/server.js",
        "dev": "nodemon -r tsconfig-paths/register ./src/server.ts"
    },
    "paths": {
        "@controllers/*": [
            "./src/controllers/*"
        ],
        "@interfaces/*": [
            "./src/interfaces/*"
        ],
        "@middleware/*": [
            "./src/middleware/*"
        ],
        "@models/*": [
            "./src/models/*"
        ],
        "@helpers/*": [
            "./src/helpers/*"
        ],
        "@routes/*": [
            "./src/routes/*"
        ]
    }
}
